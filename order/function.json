{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "message",
      "type": "queue",
      "direction": "out",
      "queueName": "worker",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
